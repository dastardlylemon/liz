{
  "manifest_version": 2,
  "name": "Liz",
  "version": "0.1",

  "description": "and the blue bird",

  "content_scripts": [
    {
      "matches": [
        "*://*.twitter.com/*"
      ],
      "js": ["liz.js"],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    "square-avatars.css",
    "hide-moments.css",
    "liked-tweets.css",
    "small-media.css",
    "show-links.css"
  ]
}
